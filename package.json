{
  "name": "webeleon-node-typescript-starter",
  "version": "0.0.0",
  "description": "A generic starter for node js starter projects",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/index.js",
    "format": "prettier --write '**/*.ts'",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "prepare": "husky install",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Webeleon/node-typescript"
  },
  "bugs": {
    "url": "https://github.com/Webeleon/node-typescript/issues"
  },
  "contributors": [
    {
      "name": "Julien Prugne",
      "email": "julien@webeleon.dev"
    }
  ],
  "keywords": [
    "project starter",
    "webeleon",
    "node js",
    "typescript"
  ],
  "author": "Webeleon",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "13.2.1",
    "@commitlint/config-conventional": "13.2.0",
    "@types/jest": "27.0.2",
    "@types/sinon": "10.0.4",
    "husky": "7.0.2",
    "jest": "27.2.5",
    "lint-staged": "11.2.3",
    "prettier": "2.4.1",
    "sinon": "11.1.2",
    "standard-version": "9.3.1",
    "ts-jest": "27.0.6",
    "typescript": "4.4.4"
  },
  "lint-staged": {
    "src/**/*": "prettier --write --ignore-unknown"
  }
}
